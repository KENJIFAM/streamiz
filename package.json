{
  "name": "streams",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack -d --watch --config webpack.config.js",
    "build:prod": "webpack -p --config webpack.server.config.js && webpack -p --config webpack.config.js",
    "start": "webpack -d && lite-server",
    "db": "json-server -p 4000 -w db.json",
    "rtmp": "nodemon rtmpserver.js",
    "dev": "concurrently --kill-others-on-fail \"npm run build\" \"npm run db\" \"npm run rtmp\" \"lite-server\"",
    "build:server": "webpack -d --watch --config webpack.server.config.js",
    "dev:server": "concurrently -k -p \"[{name}]\" -n \"WebpackServer,WebpackClient,RTMP,Node\" -c \"yellow.bold,blue.bold,green.bold,red.bold\" \"npm run build:server\" \"npm run build\" \"npm run rtmp\" \"nodemon dist/server.js\""
  },
  "keywords": [],
  "author": "Kenji Fam",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "flv.js": "^1.5.0",
    "json-server": "^0.14.1",
    "mongoose": "^5.4.5",
    "node-media-server": "^1.4.9",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-form": "^8.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
