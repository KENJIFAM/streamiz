    "@types/mongoose": "^5.3.8",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "@types/redux-form": "^7.4.13",
    "@types/redux-thunk": "^2.1.0",
    "concurrently": "^4.1.0",
    "lite-server": "^2.4.0",
    "nodemon": "^1.18.9",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
