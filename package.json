{
  "name": "streams",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "private": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "build": "npm run build:prod && npm run tslint",
    "build:prod": "webpack -p --config webpack.server.config.js && webpack -p --config webpack.config.js",
    "build:server": "webpack -d --watch --config webpack.server.config.js && npm run tslint",
    "build:client": "webpack -d --watch --config webpack.config.js && npm run tslint",
    "rtmp:dev": "nodemon rtmpserver.js",
    "serve": "node dist/server.js",
    "serve:dev": "nodemon dist/server.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"WebpackServer,WebpackClient,RTMP,Node\" -c \"yellow.bold,blue.bold,green.bold,red.bold\" \"npm run build:server\" \"npm run build:client\" \"npm run rtmp:dev\" \"npm run serve:dev\"",
    "start": "npm run serve",
    "prepare": "npm run build",
    "deploy": "gcloud app deploy"
  },
  "keywords": [],
  "author": "Kenji Fam",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "flv.js": "^1.5.0",
    "helmet": "^3.15.0",
    "mongoose": "^5.4.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-form": "^8.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/helmet": "0.0.42",
    "@types/json-server": "^0.14.0",
    "@types/mongoose": "^5.3.8",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "@types/redux-form": "^7.4.13",
    "@types/redux-thunk": "^2.1.0",
    "css-loader": "^2.1.0",
    "dotenv-webpack": "^1.7.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-media-server": "^1.4.10",
    "nodemon": "^1.18.9",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
